#include "pch.h"
#include <iostream>
#include <string>
#include <math.h>
using namespace std;
int main()
{

	float a, b, z, t;
	a = -0.6;
	b = 5.3;
	if (a < b)
	{
		z = abs(((a*a) - (b*b)));
		z = pow(z, 0.5);
	}
	z = 1 - (2 * cos(a)*sin(b));
	if (z < b)
	{
		t = z + a * a*b;
		t = pow(t, 1.0 / 3);
	}
	else
		if (z = b)
		{
			z = 1 - log10(z) + cos(a*a*b);
		}
	t = 1 / (cos(z*a)*sin(b));
	cout << "a=" << a << " " << "b=" << b << " " << "z=" << z << " " << "t=" << t;


	return 0;
}
