#include "pch.h"
#include <iostream>
#include <string>
#include <math.h>
using namespace std;
int main()
{

	float a, b, y, t;
	a = 4.6;
	b = 1.5;
	if (a <= b)
	{
		y = ((a - b) / (a + b))*((a + b) / ((a*a )-( a * b) + (b * b)));
	}
	y = a+ log(fabs(b*b));
	if (y=b)
	{
		t = ((2 * y) + pow(((y*y) - a), 0.5)) / ((2 * b) - pow(((a*a) - y), 0.5));
	}
	else
		if (y < b)
		{
			t = sin(y)*sin(y) + (1 /(tan(a - b)));
		}
	t = (pow((y*sin(a)),1.0 / 3)) + (1 / pow((y*cos(b)),0.5));
	cout << "a=" << a << " " << "b=" << b << " " << "y=" << y << " " << "t=" << t;

	return 0;
}
