#include "pch.h"
#include <iostream>
#include <string>
#include <math.h>
using namespace std;
int main()
{

	float a, x, y, t;
	a = 4.6;
	x = 1.5;
	if (a <= x)
	{
		y = a+log(fabs(x+a));
	}
	y = pow(sin(a*x),0.5);
	if (a>y)
	{
		t = y/(a-x);
	}
	else
		if (a=y)
		{
			t =( y / (a - x))+((a+x)/(y*y));
		}
	t = (tan(a*x))+(cos(2*a*y));
	cout << "a=" << a << " " << "x=" << x << " " << "y=" << y << " " << "t=" << t;

	return 0;
}
